[
  {
    "description": "这是一个带 header 信息的 post 请求",
    "request": {
      "uri": "/post/with/header",
      "method": "post",
      "headers": {
        "content-type": "application/json"
      },
      "json": {
        "name": "leo",
        "age": "11"
      }
    },
    "response": {
      "json": {
        "leo": "success",
        "status": "202"
      }
    }
  }
]